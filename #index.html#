<script type="text/javascript">
  function displayNextImage() {
  x = (x === images.length - 1) ? 0 : x + 1;
  document.getElementById("imag").src = "NWEJM_fichiers/" + images[x];
  document.getElementById("imag").title = comments[x];
  document.getElementById("ImageCaption").innerHTML = comments[x];
  }
  
  function startTimer() {
            setInterval(displayNextImage, 4000);
  }

  var images = [], comments = [], x = -1; 
  images[0] = "Place-1.jpg"; comments[0] = "The Lille Main Square";
  images[1] = "Place-2.jpg"; comments[1] = "The Brussels Atomium";
  images[2] = "Place-3.jpg"; comments[2] = "The Turku Castle in Finland";
  images[3] = "Place-4.jpg"; comments[3] = "The London House of Parliament";
  images[5] = "Place-6.jpg"; comments[5] = "Amsterdam";
  images[4] = "Place-5.jpg"; comments[4] = "The Berlin Bandenburg Gate";
  images[6] = "Place-7.jpg"; comments[6] = "Stockholm";
  images[7] = "Place-9.jpg"; comments[7] = "The Bergen Fjord in Norway";
  images[8] = "Place-8.jpg"; comments[8] = "Munich";
</script>
